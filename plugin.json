{
    "description": "Monitors Windows Process Metrics",
    "command": "powershell -NoLogo -NonInteractive -Command Set-ExecutionPolicy RemoteSigned; .\\process-metric.ps1 CPU $(process)",
    "metrics" : [
    "WINDOWS_PROCESS_CPU"
    ],

    "paramSchema": [
        {
            "title": "Process",
            "name": "process",
            "description": "Name of the process to monitor",
            "type": "string",
            "default": "powershell",
            "required": true
        }
    ]
}

